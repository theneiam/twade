{
    "name": "theneiam/twade",
    "description": "View engine plugin for Seotoaster 2.x, that brings Twig and Blade power to the system",
    "keywords":["seotoaster","twig","blade"],
    "type":"library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require" : {
        "php": ">=5.4",
        "illuminate/view": "4.2.*",
        "twig/twig": "1.*",
        "mustache/mustache": "2.6.1",
        "twig/extensions": "1.1.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Twade\\Tests": "tests"
        },
        "psr-4": {
            "Twade\\Engine\\": "src"
        }
    },
    "authors": [
        {
            "name": "Eugene I. Nezhuta",
            "email": "eugene.nezhuta@gmail.com"
        }
    ]
}